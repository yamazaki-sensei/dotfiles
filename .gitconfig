[core]
	excludesfile = ~/.gitignore_global
	editor = /usr/bin/vim
[advice]
  detachedHead = false
[user]
	name = hira
	email = almichest.free@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[alias]
	p  = !git push -u origin `git rev-parse --abbrev-ref HEAD`
	co = checkout
	cm = commit
	d  = diff
	s  = status
	l  = log
  br = branch
  rb = rebase
  f = fetch
  pr = "!git p; sh ~/create_git_pr.sh"
  prn = "!sh ~/create_git_pr.sh"
  del = "!git br | grep -v \"\\*\" | xargs git br -D"
  co-d = "!git fetch; git co origin/develop; git co -b"
  co-m = "!git fetch; git co origin/master; git co -b"

[fetch]
	prune = true
[credential]
	helper = osxkeychain
